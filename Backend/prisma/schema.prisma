generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["interactiveTransactions"]
}

generator docs {
    provider = "node node_modules/prisma-docs-generator"
    output   = "../docs/schema"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Session {
    id        String   @id
    sid       String   @unique
    data      String
    expiresAt DateTime
}

model User {
    matricula String @id
    email     String @unique
    password  String
}
